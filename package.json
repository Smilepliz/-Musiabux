{
  "name": "musiabux",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "build:exe": "npm run build && node node_modules/electron-builder/cli.js --win"
  },
  "dependencies": {
    "docx": "^9.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "typescript": "~5.6.2",
    "vite": "^5.4.10"
  },
  "build": {
    "appId": "com.musiabux.app",
    "productName": "Musiabux",
    "files": ["dist", "electron"],
    "directories": { "output": "release" },
    "win": {
      "target": "portable"
    }
  }
